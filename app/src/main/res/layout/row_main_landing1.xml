                android:id="@+id/txtCottonblackdr"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{mainLanding1RowModel.txtCottonblackdr}"
                style="@style/txtLatoregular12_1" tools:text="@string/msg_cotton_black_dr"/>
        </LinearLayout>
    </LinearLayout>
</layout>
