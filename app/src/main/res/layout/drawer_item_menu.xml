<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemMenuModel" type="com.nemanjasapplication.app.modules.itemmenu.data.model.DrawerItemMenuModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumneye"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900_90">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearMenu"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_80pxh"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <ImageView android:id="@+id/imageEye"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_34pxv"
                    android:layout_marginStart="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_784pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_eye"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumncart"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_33pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_70pxv"
                    android:layout_weight="0.63"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtCART"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtCART}"
                        style="@style/txtLatoregular18" tools:text="@string/lbl_cart"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtPROFILE"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtPROFILE}"
                        style="@style/txtLatoregular18" tools:text="@string/lbl_profile"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtClothing"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtClothing}"
                        style="@style/txtLatoregular18" tools:text="@string/lbl_clothing"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtShoes"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{drawerItemMenuModel.txtShoes}"
                        style="@style/txtLatoregular18" tools:text="@string/lbl_shoes"/>
                    <LinearLayout android:id="@+id/linearRowfacebook"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_538pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageFacebook"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_facebook"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                        <ImageView android:id="@+id/imageSettings"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_settings"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_34pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_784pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
